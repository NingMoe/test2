count = $item['qty'];\n    \n    $sql = \"SELECT * FROM \" .$ecs->table('hpyer_sign_log'). \" WHERE uid='{$user_id}' ORDER BY id DESC LIMIT {$start}, {$limit}\";\n    $res = $db->query($sql);\n    $list = array();\n    while ($item = $db->fetch_array($res)) {\n    	$item['add_time'] = local_date('Y-m-d H:i:s', $item['add_time']);\n    	$list[] = $item;\n    }\n    \n    $pager  = get_pager('user.php', array('act' => $action), $record_count, $page, $limit);\n    $smarty->assign('pager',  $pager);\n    $smarty->assign('list', $list);\n    $smarty->display('user_clips.dwt');\n}\n\n\n二、模板\n签到按钮的样式请自行设计。\n\n1、需要放置签到按钮的页面，需要加入如下代码：\n{insert_scripts files='hpyer.js'}\n\n2、签到按钮的点击事件，设置为：hpyer_sign(); 即可。如：<input type=\"button\" onclick=\"hpyer_sign();\" value=\"马上签到\" />\n\n3、模板文件 user_clips.dwt 第42行（<div class=\"userCenterBox boxCenterList clearfix\" style=\"_height:1%;\">）下方加入下面代码：\n（这里是默认模板，其他模板请自行参照修改）\n\n<!-- *签到日志 start-->\n<!-- {if $action eq 'sign_log_list'} -->\n<h5><span>{$lang.label_sign_log_list}</span></h5>\n<div class=\"blank\"></div>\n<table width=\"100%\" border=\"0\" cellpadding=\"5\" cellspacing=\"1\" bgcolor=\"#dddddd\" id=\"order_table\">\n<tr align=\"center\">\n  <th bgcolor=\"#ffffff\">{$lang.sign_time}</th>\n  <th bgcolor=\"#ffffff\">{$lang.get_points}</th>\n</tr>\n<!--{foreach from=$list item=item}-->\n<tr>\n  <td align=\"center\" bgcolor=\"#ffffff\">{$item.add_time|date_format:\"Y/m/d\"}</td>\n  <td align=\"center\" bgcolor=\"#ffffff\">{$item.points}</td>\n</tr>\n<!--{/foreach}-->\n</table>\n<div class=\"blank5\"></div>\n<!-- #BeginLibraryItem \"/library/pages.lbi\" --><!-- #EndLibraryItem -->\n<!-- {/if} -->\n<!-- #签到日志 end-->\n\n4、模板文件 library/user_menu.lbi 第20行（<a href=\"user.php?act=logout\" style=\"background:none; text-align:right; margin-right:10px;\"><img src=\"../images/bnt_sign.gif\"></a>）上方加入下面代码：\n（这里是默认模板，其他模板请自行参照修改）\n\n<!--{if $hpyer_setting.sign.save_sign_log}-->\n<a href=\"user.php?act=sign_log_list\"{if $action eq 'sign_log_list'}c